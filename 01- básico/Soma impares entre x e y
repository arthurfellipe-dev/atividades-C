#include <stdio.h>

int main() {
    
    int inicio, fim, soma = 0;

    scanf("%d", &inicio);
    scanf("%d", &fim);

    for (int i = inicio + 1; i < fim; i++) {
        if (i % 2 != 0) {
            printf("%d\n", i);
            soma += i;
        }
    }

    printf("\nSoma = %d\t", soma);

    return 0;
}
