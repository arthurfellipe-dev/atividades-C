#include <stdio.h>
int main() {
    int ovo, tipo[2] = {0,0};
     for(int i = 1; i <=5;i++){
         for(int j = 1; j <=4;j++){
             scanf("%d", &ovo);
        if (ovo % 3 == 0)
            tipo[1]++;
          else
            tipo[0]++;
         }
     }
  printf("ovos normais: %d", tipo[0]);
  printf("ovos dourados: %d", tipo[1]);
    return 0;
}
