#include <stdio.h>
int main() {
    char quebrado;
    int i, j, descartar;

    for(i = 1; i <= 4; i++) {
        descartar = 0; 
        for(j = 1; j <= 6; j++) {
            scanf(" %c", &quebrado); 
            if(quebrado == 'Q')
                descartar++;
        }
        if(descartar > 2)
            printf("Lote %d foi descartado\n", i);
    }
    return 0;
}
